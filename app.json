{
  "expo": {
    "name": "Teo: Daily Accountability",
    "slug": "teo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#0F172A",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.teo.app",
      "backgroundColor": "#0F172A",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.teo.app",
      "backgroundColor": "#0F172A",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share check-in photos."
        }
      ],
      "expo-secure-store"
    ],
    "scheme": "teo",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bcfc5ecd-053a-4fc1-9f2f-183302d297a3"
      }
    }
  }
}
